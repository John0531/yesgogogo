.item{
  width: 35%;
  position: absolute;
  top: 17%;
  z-index: 3;
  @media (min-width: 992px) {
    top: 20%;
    width: 20%;
  }
}

.foodmove1{
  animation: bannermove1 16s infinite;
}

@keyframes bannermove1 {
  0% { left: -5%; transform: translateY(10rem); }
  17% { left: -25%; transform: translateY(18rem); }
  33% { left: -50%; transform: translateY(26rem); }
  50% { left: -75%; transform: translateY(34rem); }
  67% { left: -100%; transform: translateY(42rem); }
  83% { left: -125%; transform: translateY(50rem); }
  100% { left: -150%; transform: translateY(58rem); }
}

