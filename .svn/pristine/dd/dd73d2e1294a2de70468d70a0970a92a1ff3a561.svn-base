            title: `20220101${res.data.rtnMsg}(${res.data.rtnCode})`,
            allowOutsideClick: false,
            confirmButtonColor: '#F8412E',
            confirmButtonText: '確認',
            width: 400,
            customClass: {
              title: 'text-class',
              confirmButton: 'confirm-btn-class'
            }
          })
          return
        }
        this.giftList = res.data.info
        this.giftList.forEach(item => {
          if (item.code === '20220101_A') {
            this.A = item.products /* 期間限定閃購專區 20220101_A  */
          } else if (item.code === '20220101_B') {
            this.B = item.products /* 即時料理，家常菜 20220101_B */
          } else if (item.code === '20220101_C') {
            this.C = item.products /* 鍋物湯品 20220101_C */
          } else if (item.code === '20220101_D') {
            this.D = item.products /* 水餃麵點 20220101_D */
          } else if (item.code === '20220101_E') {
            this.E = item.products /* 蛋糕甜點 20220101_E  */
          } else if (item.code === '20220101_F') {
            this.F = item.products /* 生鮮肉品，小農蔬果 20220101_F  */
          }
        })
      })
    },
    getCardDiscount () {
      // ?card_A 週二聯邦日活動代碼
      const url = `${process.env.VUE_APP_API}/api/widgets/activitybanner?code=card_A`
      this.$http.get(url).then(res => {
        if (res.data.rtnCode !== 0) {
          this.$swal.fire({
            title: `card_A${res.data.rtnMsg}(${res.data.rtnCode})`,
            allowOutsideClick: false,
            confirmButtonColor: '#F8412E',
            confirmButtonText: '確認',
            width: 400,
            customClass: {
              title: 'text-class',
              confirmButton: 'confirm-btn-class'
            }
          })
          return
        }
        if (res.data.rtnCode === 0) {
          this.card = res.data.info
        }
      })
    },
    goLatestAct () {
      // ?card_B 當月最新優惠活動按鈕WEb
      const url = `${process.env.VUE_APP_API}/api/widgets/activitybanner?code=card_C`
      this.$http.get(url).then(res => {
        if (res.data.rtnCode !== 0) {
          this.$swal.fire({
            title: `card_C${res.data.rtnMsg}(${res.data.rtnCode})`,
            allowOutsideClick: false,
            confirmButtonColor: '#F8412E',
            confirmButtonText: '確認',
            width: 400,
            customClass: {
              title: 'text-class',
              confirmButton: 'confirm-btn-class'
            }
          })
          return
        }
        if (res.data.rtnCode === 0) {
          this.carousel = res.data.info
        }
      })
      const urlM = `${process.env.VUE_APP_API}/api/widgets/activitybanner?code=card_m_C`
      this.$http.get(urlM).then(res => {
        if (res.data.rtnCode !== 0) {
          this.$swal.fire({
            title: `card_m_C${res.data.rtnMsg}(${res.data.rtnCode})`,
            allowOutsideClick: false,
            confirmButtonColor: '#F8412E',
            confirmButtonText: '確認',
            width: 400,
            customClass: {
              title: 'text-class',
              confirmButton: 'confirm-btn-class'
            }
          })
          return
        }
        if (res.data.rtnCode === 0) {
          this.carousel_mb = res.data.info
        }
      })
    },
    goHyperText (DOM) {
      // ?前往茅點
      const domHeight = DOM.offsetTop
      setTimeout(() => {
        window.scrollTo(0, domHeight)
      }, 500)
    },
    getToken () {
      // ?呼叫checkToken.js
      // ?判斷是否有登入(token取得)
      const accessToken = checkToken.getAccessToken()
      const freshToken = checkToken.getFreshToken()
      if (accessToken || freshToken) {
        return true
      } else {
        return false
      }
    },
    goEventGiftPath () {
      // ?呼叫 event.js確認活動碼
      getGiftEvent().then(data => {
        if (!data.isSuccess) {
          this.$swal.fire({
            title: `${data.message}(${data.rtnCode})`,
            allowOutsideClick: false,
            confirmButtonColor: '#F8412E',
            confirmButtonText: '確認',
            width: 400,
            customClass: {
              title: 'text-class',
              confirmButton: 'confirm-btn-class'
            }
          })
        } else {
          this.$swal.fire({
            title: `${data.message}`,
            allowOutsideClick: false,
            confirmButtonColor: '#F8412E',
            confirmButtonText: '確認',
            width: 400,
            customClass: {
              title: 'text-class',
              confirmButton: 'confirm-btn-class'
            }
          })
        }
        // this.$router.push(sessionStorage.getItem('path'))
        // 活動結束，清除所有sessionStorage
        sessionStorage.removeItem('cardEvent')
        sessionStorage.removeItem('cardCode')
        sessionStorage.removeItem('path')
      })
    },
    goUpdatePwd () {
      sessionStorage.setItem('cardEvent', 'card')
      if (this.getToken()) {
        this.$router.push('/memberboard/memberupdatepwd')
      } else {
        if (document.body.clientWidth < 576) {
          // ?手機板
          this.$router.push('/membermobilelogin')
        } else {
          // ? web 版
          this.$router.push('/memberboard/memberupdatepwd/?cardEvent=card')
        }
      }
    }
  },
  watch: {
    $route (to, from) {
      if (
        to.fullPath.match('/productboard/product/') ||
        to.fullPath.match('/productboard/productList/')
      ) {
        const recordHeight = window.scrollY
        this.$store.commit('getEventRecordHeight', recordHeight)
      } else {
        this.$store.commit('getEventRecordHeight', '')
      }
    }
  },
  mounted () {
    this.getGiftList()
    this.goLatestAct()
    this.getCardDiscount()
    // ? 若有點擊產品紀錄，則記錄高度供上一頁返回
    if (this.$store.state.recordEventReadHeight) {
      setTimeout(
        () => [
          // ?資料高度生成需要時間，利用setTimeOut讓視窗滑動不同步，才能順利執行
          window.scrollTo(0, this.$store.state.recordEventReadHeight)
        ],
        500
      )
    }
    // ? 從Line進入yesgo紀錄query參數
    if (
      Object.keys(this.$route.query).length !== 0 &&
      this.$route.query.path &&
      this.$route.query.cardCode
    ) {
      Object.keys(this.$route.query).forEach(item => {
        sessionStorage.setItem(`${item}`, this.$route.query[item])
      })
      if (this.getToken()) {
        this.goEventGiftPath()
      } else {
        this.$swal.fire({
          title: '聯邦卡友會員您好~<br>請登入參加月月抽百杯CITY CAFE活動',
          allowOutsideClick: true,
          confirmButtonColor: '#F8412E',
          confirmButtonText: '確認',
          backdrop: true,
          width: 400,
          customClass: {
            title: 'text-class',
            confirmButton: 'confirm-btn-class'
          }
        })
        if (document.body.clientWidth < 576) {
          // ? 手機版
          sessionStorage.setItem('cardEvent', 'card')
          this.$router.push('/membermobilelogin')
        } else {
          // ? web 版
          sessionStorage.setItem('cardEvent', 'card')
          // 因首頁判斷過多，多塞一個cardEvent供活動判斷
          this.$router.push('/?cardEvent=card')
        }
      }
    }
  }
}
</script>

